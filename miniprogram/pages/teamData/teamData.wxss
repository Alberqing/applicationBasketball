/* pages/teamData/teamData.wxss */

.data {
    font-size: 16px;
    margin: 5px;
}

ec-canvas {
    width: 100%;
    height: 500px;
    margin-top: 5px;
}

van-row {
    font-size: 14px;
    display: block;
    width: 95%;
    height: 100%;
    margin: 0 auto;
}

van-row:first-child {
    margin-top: 10px;
}

.rich-text {
    display: block;
    font-size: 14px;
    color: #1a1818;
    margin: 10px 5px;
    background: rgb(223, 223, 223);
    padding-left: 2em;
    border-radius: 8px;
    box-shadow: 10px 10px 5px #888;
}

.text {
    font-size: 14px;
    margin: 5px;
}
